<h1>フィナンシャルプランナー様　新規ご登録</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
  	<%= form_with model: @planner, local: true do |f| %>
  		
  		<%= render 'shared/error_messages', object: f.object %>
  		
  		<%= f.label :name %>
  		<%= f.text_field :name, class: 'form-control' %>
  		
  		<%= f.label :email %>
  		<%= f.email_field :email, class: 'form-control' %>
  		
  		<%= f.label :password %>
  		<%= f.password_field :password, class: 'form-control' %>
  		
  		<%= f.label :password_confirmation, "Confirmation" %>
  		<%= f.password_field :password_confirmation, class: 'form-control' %>
  		
  		<%= f.label :skill, "スキル" %>
  		  <%= f.collection_check_boxes(:skill_ids, @skills, :id, :name) do |sk| %>
  		    <%= sk.label { sk.check_box + sk.text } %>
  		  <% end %>
  		<%= f.submit "新規作成", class: "btn btn-primary" %>
  	<% end %>
  </div>
</div>

<%= link_to "トップに戻る", root_url %>